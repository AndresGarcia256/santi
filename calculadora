
package javaapplication6;
import java.util.Scanner;

public class JavaApplication6 {
    public static void main(String[] args) {
        Scanner  objetoentrada=new Scanner (System.in);
        System.out.println("Cuantos objetos iran en la lista?");
        int ab=objetoentrada.nextInt();
        int lista[]=new int[ab];
        for (int i=0;i<ab;i++){
            System.out.println("digite un valor a ser agregado a la lista");
            int a=objetoentrada.nextInt();  
            lista[i]=a;
            }
        for (int i=0;i<lista.length;i++)
            {
            System.out.println("la lista es "+ lista[i]);
            }
    }
    
}
